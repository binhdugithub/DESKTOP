<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="eVMS.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <!-- <log4net configSource="log4net.config" /> -->
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="LogTest2.txt" />
      <param name="AppendToFile" value="true" />
      <maxSizeRollBackups value="0" />
      <maximumFileSize value="1KB" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="login" value="api/evms/login" /> <!-- login-->
    <add key="addnewuser" value="api/evms/user" /> <!--Add new user-->
    <add key="getrole" value="api/evms/role"/> <!--Get all user role-->
    <add key="deluser" value="api/evms/user"/> <!--delete a user-->
    <add key="getalluser" value="api/evms/user"/>
    <add key="getlistvmsbyuser" value="/api/evms/vms"/> <!--get list vms by user-->
    <add key="assignvmstouser" value="/api/evms/user/vms"/>
    <add key="addnewvms" value="/api/evms/vms"/> <!--create a new vms device-->
    <add key="delvms" value="/api/evms/vms"/> <!--delete a vms-->
    <add key="updatevms" value="/api/evms/vms"/><!--update a vms-->
    <add key="updatetreevms" value="api/evms/vms/program"/> <!--update tree vms to server-->
    <add key="saveprogam" value="/api/evms/program"/> <!--Save to program (template)-->
    <add key="updateprogam" value="/api/evms/program"/> <!--Save to program (template)-->
    
    <add key="getlistprogram" value="/api/evms/program"/>
    <add key="delprogram" value="/api/evms/program"/> <!-- Xoa chuong trinh-->
    <add key="getdetailprogram" value="/api/evms/program"/>

    <add key="getlistvmsmonitoring" value="/api/evms/monitoring"/>
    <add key="getvmsmodellist" value="/api/evms/vmsmodel"/>
    <add key="getlistimagesource" value="/api/evms/imagesource"/>
    <add key="getlisthistory" value="/api/evms/history"/>
    <!-- Help file-->
    <add key="helpfile" value="\\Help\\eVms guide.chm"/>
    <!--End Help file-->
  </appSettings>
  <userSettings>
    <eVMS.Properties.Settings>
      <setting name="ServerIp" serializeAs="String">
        <value />
      </setting>
      <setting name="Port" serializeAs="String">
        <value />
      </setting>
    </eVMS.Properties.Settings>
  </userSettings>
</configuration>